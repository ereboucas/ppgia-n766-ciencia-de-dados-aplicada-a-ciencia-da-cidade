    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Parameters\n",
    "n_steps = 100\n",
    "\n",
    "x = [0] * n_steps\n",
    "for i in range(1, n_steps):\n",
    "    x[i] = x[i - 1] + [1, -1][np.random.randint(0, 2)]\n",
    "\n",
    "plt.plot(x)\n"
